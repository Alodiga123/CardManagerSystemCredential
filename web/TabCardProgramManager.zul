<?xml version="1.0" encoding="UTF-8"?>
<?taglib uri="/WEB-INF/tld/core.dsp.tld" prefix="c" ?>
<?page title="${c:l('sp.common.page.title')}"?>
<?link rel="icon" tye="image/x-icon" href="/images/favicon.ico"?>
<?init class="com.alodiga.cms.web.security.IndexAdminRedirectInit" ?>

<zk xmlns:n="native">
    <window id="winTabCardProgramManager" width="100%" height="100%" apply="com.alodiga.cms.web.controllers.AdminCardProgramManagerController" class="general-window">
        <style src="/css/general.css"/>
        <borderlayout width="100%" height="100%" class="content-transparent">
            <north border="none" size="52px" class="content-transparent">
                <include src="adminHeader.zul"/>
            </north>
            <center border="none" flex="true" class="content-transparent" autoscroll="true">
                <div align="center" >
                    <hlayout >
                        <div></div>
                        <div width="1200px" align="left" height="780px">
                            <hlayout>
                                <div>
                                    <include src="adminMainMenu.zul"/>
                                </div>
                                <div width="900px">
                                    <vlayout>
                                        <div height="20px" width="100%" align="left">
                                            <!--Bread crumb-->
                                            <hlayout>
                                                <toolbarbutton label="${c:l('sp.bread.crumb.home')}" onClick='Executions.sendRedirect(&quot;home-admin.zul&quot;)'/>
                                                |
                                                <toolbarbutton label="${c:l('cms.common.legalPerson')}" onClick="Executions.sendRedirect(&quot;listCardProgramManager.zul&quot;)"/>
                                                |
                                                <toolbarbutton id= "tbbTitle" label="${c:l('cms.crud.card.manager.add')}"/>
                                            </hlayout>
                                        </div>
                                        <div height="5px" width="100%" style="background:#333;" align="left"></div>
                                        <div>
                                            <vlayout>
                                                <div align="right">
                                                    <hlayout>
                                                        <label value="${c:l('sp.common.field.required1')}" sclass="lblexample" />
                                                        <separator/>
                                                        <label value="*" sclass="lblreq" />
                                                        <separator/>
                                                        <label value="${c:l('sp.common.field.required2')}" sclass="lblexample" />
                                                    </hlayout>
                                                </div>
                                                <tabbox id="tbxCardProgramManagerTabs" width="970px" height="490px">
                                                    <tabs>
                                                        <tab id="tabMain" label="${c:l('cms.tab.CardProgramManagerMain')}" />
                                                        <tab id="tabAddressCardProgramManager" label="${c:l('cms.common.address')}"/>
                                                        <tab id="tabLegalRepresentativesCardProgramManager" label="${c:l('cms.tab.legalRepresentativesCardProgramManager')}" />
                                                    </tabs>
                                                    <tabpanels>
                                                        <tabpanel>
                                                            <include src="adminCardProgramManager.zul"/>
                                                        </tabpanel>
                                                        <tabpanel>
                                                            <include src="adminAddressCardProgramManager.zul"/>
                                                        </tabpanel>
                                                        <tabpanel>
                                                            <include src="listLegalRepresentativesCardProgramManager.zul"/>
                                                        </tabpanel>
                                                    </tabpanels>
                                                </tabbox>
                                            </vlayout>
                                        </div>
                                        <div id="divInfo" height="20px" width="100%" style="background:#B6E59E;" align="left" visible="false"/>
                                    </vlayout>
                                </div>
                                <div></div>
                            </hlayout>
                        </div>
                        <div></div>
                    </hlayout>
                </div>
            </center>
            <south border="none" size="40px">
                <include src="adminFooter.zul"/>
            </south>
        </borderlayout>
    </window>
</zk>
